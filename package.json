{
  "name": "lcm-requester",
  "version": "2.1.0",
  "description": "lcm-requster is a wrapper on request module with validation on top of format and rules used",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --opts tests/mocha.opts -R spec ./tests",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha  --print both -- --opts tests/mocha.opts -R spec ./tests"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "dns-lookup-cache": "^1.0.0",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "chai": "^4.1.1",
    "data-driven": "^1.3.0",
    "eslint": "^4.6.1",
    "get-port": "^3.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "proxyquire": "^1.8.0",
    "sinon": "^3.2.1"
  }
}
